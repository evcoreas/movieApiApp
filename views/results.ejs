<% include partials/header %>

<div class="header">
	<h1>Results</h1>
</div>

<div class="container">
	<div class="row">
	<% dataVar["Search"].forEach(function(movie) { %>
			<img src= "<%= movie["Poster"] %>" >
			<div class="title">
				<strong>
				<%= movie["Title"] %>
				</strong>
				 - <%= movie["Year"] %>
			 </div>
	<% }) %>
	</div>
</div>


<button><a href="/">Search Again!</a></button>

<%include partials/footer %>